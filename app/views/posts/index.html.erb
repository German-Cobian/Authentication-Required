<h1>Posts#index</h1>


<ul>
  <% @posts.each do |post| %>
   <li>
       <%= post.title %>
    </li>
  <% end %>
</ul>


<ul>
  <% @users.each do |user| %>
    <% if user_signed_in? %>
      <li>
        <%= user.username %>
      </li>
    <% else %>
      <li>
        ***
      </li>
    <% end %>
  <% end %>
</ul>
